{
  "dashboard": {
    "id": null,
    "title": "Ecom Ecosystem - Production Dashboard",
    "tags": ["ecommerce", "production", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Application Status",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "up{job=\"ecom-app\"}",
            "legendFormat": "App Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"options": {"0": {"text": "DOWN", "color": "red"}}, "type": "value"},
              {"options": {"1": {"text": "UP", "color": "green"}}, "type": "value"}
            ],
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Response Time (95th percentile)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 2}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "rate(http_requests_total{status=~\"5..\"}[5m]) * 100",
            "legendFormat": "Error Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Active Users",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "active_users_total",
            "legendFormat": "Active Users"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        }
      },
      {
        "id": 5,
        "title": "Request Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{status}}"
          }
        ],
        "yAxes": [
          {"label": "Requests/sec", "min": 0}
        ]
      },
      {
        "id": 6,
        "title": "Memory Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
        "targets": [
          {
            "expr": "nodejs_memory_heap_used_bytes / 1024 / 1024",
            "legendFormat": "Heap Used (MB)"
          },
          {
            "expr": "nodejs_memory_heap_total_bytes / 1024 / 1024",
            "legendFormat": "Heap Total (MB)"
          }
        ],
        "yAxes": [
          {"label": "Memory (MB)", "min": 0}
        ]
      },
      {
        "id": 7,
        "title": "Business Metrics - Orders",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "rate(orders_total[1h])",
            "legendFormat": "Orders/hour"
          },
          {
            "expr": "rate(orders_completed_total[1h])",
            "legendFormat": "Completed Orders/hour"
          }
        ],
        "yAxes": [
          {"label": "Orders", "min": 0}
        ]
      },
      {
        "id": 8,
        "title": "Revenue",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 12},
        "targets": [
          {
            "expr": "rate(revenue_total[1h])",
            "legendFormat": "Revenue/hour"
          }
        ],
        "yAxes": [
          {"label": "Revenue ($)", "min": 0}
        ]
      },
      {
        "id": 9,
        "title": "Cart Metrics",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 12},
        "targets": [
          {
            "expr": "rate(cart_created_total[1h])",
            "legendFormat": "Carts Created/hour"
          },
          {
            "expr": "rate(cart_abandoned_total[1h])",
            "legendFormat": "Carts Abandoned/hour"
          }
        ],
        "yAxes": [
          {"label": "Carts", "min": 0}
        ]
      },
      {
        "id": 10,
        "title": "Database Performance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "database_query_duration_seconds",
            "legendFormat": "Query Duration"
          },
          {
            "expr": "database_connections_active",
            "legendFormat": "Active Connections"
          }
        ],
        "yAxes": [
          {"label": "Duration (s) / Connections", "min": 0}
        ]
      },
      {
        "id": 11,
        "title": "Cache Performance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
        "targets": [
          {
            "expr": "rate(redis_cache_hits_total[5m])",
            "legendFormat": "Cache Hits/sec"
          },
          {
            "expr": "rate(redis_cache_misses_total[5m])",
            "legendFormat": "Cache Misses/sec"
          }
        ],
        "yAxes": [
          {"label": "Operations/sec", "min": 0}
        ]
      },
      {
        "id": 12,
        "title": "Top API Endpoints",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28},
        "targets": [
          {
            "expr": "topk(10, rate(http_requests_total[5m]))",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true, "__name__": true},
              "indexByName": {},
              "renameByName": {
                "endpoint": "Endpoint",
                "method": "Method",
                "Value": "Requests/sec"
              }
            }
          }
        ]
      },
      {
        "id": 13,
        "title": "Error Logs",
        "type": "logs",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28},
        "targets": [
          {
            "expr": "{job=\"ecom-app\", level=\"error\"}",
            "refId": "A"
          }
        ]
      }
    ],
    "templating": {
      "list": [
        {
          "name": "instance",
          "type": "query",
          "query": "label_values(up{job=\"ecom-app\"}, instance)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "interval",
          "type": "interval",
          "query": "1m,5m,10m,30m,1h",
          "current": {"text": "5m", "value": "5m"}
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "prometheus",
          "expr": "deployment_timestamp",
          "titleFormat": "Deployment",
          "textFormat": "Version: {{version}}",
          "iconColor": "blue"
        }
      ]
    }
  },
  "overwrite": true
}