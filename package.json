{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:api": "tsx server.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:api\"",
    "build": "vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "build:production": "NODE_ENV=production npm run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check": "tsc --noEmit && npm run lint",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "preview:production": "NODE_ENV=production vite preview",
    "seed": "node scripts/seed-database.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "lighthouse": "lhci autorun",
    "docker:build": "docker build -t ecom-ecosystem .",
    "docker:run": "docker run -p 80:80 -p 3001:3001 ecom-ecosystem",
    "docker:compose": "docker-compose up -d",
    "docker:compose:dev": "docker-compose -f docker-compose.dev.yml up",
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "performance:analyze": "npm run build:analyze && npm run lighthouse",
    "deploy:staging": "npm run build:production && npm run test && npm run lighthouse",
    "deploy:production": "npm run security:audit && npm run deploy:staging",
    "health:check": "curl -f http://localhost:3001/api/health || exit 1",
    "logs:app": "docker logs ecom-ecosystem",
    "logs:nginx": "docker logs nginx-proxy",
    "backup:db": "node scripts/backup-database.js",
    "restore:db": "node scripts/restore-database.js"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@supabase/supabase-js": "^2.57.2",
    "@types/gsap": "^3.0.0",
    "@types/nodemailer": "^7.0.1",
    "@types/react-router-dom": "^5.3.3",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "express": "^4.18.2",
    "framer-motion": "^12.23.15",
    "gsap": "^3.13.0",
    "jspdf": "^3.0.2",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.344.0",
    "nodemailer": "^7.0.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.8.2",
    "react-syntax-highlighter": "^15.6.6",
    "recharts": "^3.2.1",
    "redis": "^4.6.0",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.0",
    "express-slow-down": "^2.0.1",
    "morgan": "^1.10.0",
    "winston": "^3.11.0",
    "prom-client": "^15.1.0",
    "node-cron": "^3.0.3",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.0",
    "sonner": "^2.0.7",
    "uuid": "^12.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "autoprefixer": "^10.4.18",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jsdom": "^23.0.0",
    "@lhci/cli": "^0.12.0",
    "postcss": "^8.4.35",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "tailwindcss": "^3.4.1",
    "terser": "^5.44.0",
    "tsx": "^4.20.5",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-pwa": "^0.17.0",
    "vitest": "^1.0.0",
    "workbox-window": "^7.0.0"
  }
}
